<%= form_for(@article) do |f| %>

<%= f.text_field :title, placeholder: 'Enter a Title' %><br>
<%= f.text_field :author, placeholder: 'author' %>
<%= f.text_area :content, placeholder: 'wht is this thicc post about' %><br>
<%= f.file_field :cover_photo %><br>

<%= f.submit 'publish article' %>
<% end %>
